{
  "name": "modules-layout-shell",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/modules/layout-shell/src",
  "prefix": "app",
  "tags": ["type:feature", "scope:layout-shell"],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "packages/modules/layout-shell/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/modules/layout-shell/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/modules/layout-shell/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/modules/layout-shell/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
