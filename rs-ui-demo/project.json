{
  "name": "rs-ui-demo",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "rs-ui-demo/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/rs-ui-demo",
        "index": "rs-ui-demo/src/index.html",
        "main": "rs-ui-demo/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "rs-ui-demo/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "rs-ui-demo/public"
          }
        ],
        "styles": [
          "packages/modules/ui/src/lib/modules-ui.styles.scss",
          "rs-ui-demo/src/styles.css"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "rs-ui-demo:build:production"
        },
        "development": {
          "buildTarget": "rs-ui-demo:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "rs-ui-demo:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "rs-ui-demo/jest.config.ts",
        "tsConfig": "rs-ui-demo/tsconfig.spec.json"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "rs-ui-demo:build",
        "port": 4200,
        "spa": true
      }
    }
  }
}
