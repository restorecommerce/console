{
  "name": "iam-user",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/modules/iam-user/src",
  "prefix": "app",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "packages/modules/iam-user/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/modules/iam-user/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/modules/iam-user/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/modules/iam-user/jest.config.ts",
        "tsConfig": "packages/modules/iam-user/tsconfig.spec.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
