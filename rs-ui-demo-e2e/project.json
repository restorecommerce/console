{
  "name": "rs-ui-demo-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "rs-ui-demo-e2e/src",
  "tags": [],
  "implicitDependencies": ["rs-ui-demo"],
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "rs-ui-demo-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "rs-ui-demo:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "rs-ui-demo:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
