    "@zxcvbn-ts/matcher-pwned": "^3.0.4",
    "ace-builds": "^1.36.2",
    "apollo-angular": "^5.0.0",
    "caniuse-lite": "^1.0.30001605",
    "change-case": "^5.4.4",
    "dayjs": "^1.11.9",
    "fuse.js": "^7.0.0",
    "ngrx-store-localstorage": "^20.0.0",
    "ngrx-store-logger": "^0.2.4",
    "rxjs": "~7.8.0",
    "subsink": "^1.0.2",
