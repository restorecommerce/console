    "stylelint-config-standard": "^28.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "lint-staged": {
    "{apps,packages}/**/*.{ts,tsx,js,jsx,html}": [
      "echo \"[Husky] code pre-commit\"",
      "pretty-quick --staged",
      "eslint --max-warnings=0"
    ],
    "{apps,packages}/**/*.scss": [
      "echo \"[Husky] styles pre-commit\"",
      "stylelint"
    ]
  }
}
