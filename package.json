    "@ngrx/effects": "^20.1.0",
    "@ngrx/entity": "^20.1.0",
    "@ngrx/operators": "^20.1.0",
    "@ngrx/router-store": "^20.1.0",
    "@ngrx/store": "^20.1.0",
    "@ngx-loading-bar/core": "^7.0.0",
    "@nx/angular": "^22.0.2",
    "@vcl/ng-vcl": "^3.4.3",
    "@vcl/vcl": "^3.4.4",
