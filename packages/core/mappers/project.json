{
  "name": "core-mappers",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/core/mappers/src",
  "tags": ["type:mappers", "scope:shared"],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/core/mappers",
        "main": "packages/core/mappers/src/index.ts",
        "tsConfig": "packages/core/mappers/tsconfig.lib.json",
        "assets": ["packages/core/mappers/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs core-mappers {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/core/mappers/jest.config.ts"
      }
    }
  }
}
